{
  "name": "base-nuxt-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:js:fix": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint:style:fix": "stylelint **/*.{vue,css} --ignore-path .gitignore --fix",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:fix": "npm run lint:js:fix && npm run lint:style:fix",
    "test": "jest",
    "commit": "cz"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^1.0.0",
    "@nuxtjs/axios": "^5.12.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "baffle": "^0.3.6",
    "nuxt": "^2.14.7",
    "nuxt-i18n": "^6.13.12",
    "portal-vue": "^2.1.7",
    "smooth-scroll": "^16.1.3",
    "vuex-persistedstate": "^3.1.0"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.4",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/color-mode": "^2.0.0",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-config-typescript": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/svg": "^0.1.12",
    "@nuxtjs/tailwindcss": "^3.1.0",
    "@types/jest": "^26.0.12",
    "@types/smooth-scroll": "^16.1.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "@vue/test-utils": "^1.0.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "commitizen": "^4.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-emoji": "^1.2.2",
    "eslint": "^7.8.1",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-vue": "^7.0.0-beta.3",
    "eslint-plugin-vue-a11y": "0.0.31",
    "jest": "^26.1.0",
    "node-sass": "^4.14.1",
    "nuxt-property-decorator": "^2.7.2",
    "sass-loader": "^10.0.2",
    "stylelint": "^13.7.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "tailwindcss-dark-mode": "^1.1.7",
    "ts-jest": "^26.1.3",
    "vue-jest": "^3.0.6",
    "vuex-module-decorators": "^0.17.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  }
}
