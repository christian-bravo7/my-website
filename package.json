{
  "name": "christian-bravo-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:js:fix": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint:style:fix": "stylelint **/*.{vue,css} --ignore-path .gitignore --fix",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:fix": "npm run lint:js:fix && npm run lint:style:fix",
    "test": "jest",
    "test:watch": "jest --w",
    "test:clear-cache": "jest --clearCache",
    "test:update-snapshot": "jest --updateSnapshot",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nuxt/components": "^2.1.4",
    "@nuxt/content": "^1.14.0",
    "@nuxtjs/i18n": "^7.2.2",
    "@nuxtjs/sitemap": "^2.4.0",
    "baffle": "^0.3.6",
    "core-js": "^3.15.1",
    "dayjs": "^1.11.5",
    "markdown-it": "^13.0.1",
    "nuxt": "^2.15.4",
    "nuxt-property-decorator": "^2.9.1",
    "portal-vue": "^2.1.7",
    "sass": "^1.54.5",
    "sass-loader": "^10.1.1",
    "vue-server-renderer": "^2.6.12",
    "vue-template-compiler": "^2.6.12",
    "vuex-persistedstate": "^4.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/color-mode": "^2.1.1",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/svg": "^0.4.0",
    "@nuxtjs/tailwindcss": "^4.2.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/glob": "^7.2.0",
    "@types/markdown-it": "^12.2.3",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.0.5",
    "commitizen": "^4.2.5",
    "cz-emoji": "^1.3.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "eslint-plugin-vuejs-accessibility": "^1.1.0",
    "husky": "^8.0.1",
    "jest": "^27.0.5",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "^10.5.4",
    "mockdate": "^3.0.5",
    "postcss": "^8.3.5",
    "prettier": "^2.7.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^22.0.0",
    "tailwindcss-dark-mode": "^1.1.7",
    "ts-jest": "^27.0.3",
    "vue-jest": "^3.0.7"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  }
}
