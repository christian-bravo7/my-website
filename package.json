    "lint:fix": "npm run lint:js:fix && npm run lint:style:fix",
    "test": "jest",
    "test:watch": "jest --w",
    "test:clear-cache": "jest --clearCache",
    "test:update-snapshot": "jest --updateSnapshot",
    "prepare": "husky install"
  },
